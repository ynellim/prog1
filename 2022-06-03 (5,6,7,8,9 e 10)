#include <stdio.h>
#include <stdlib.h>
                 // exercicios 5, 6, 7, 8, 9 e 10. (ADAPTADO)


int main ()
{
    int w = 10, x = 5, y = 6, z = 50;
    float meuNumeroDecimal = 5.99;
    char meuCaractere = 'D';


  printf("%d\n", w + x);
  printf("%d\n", x + y + z);
  printf("%d\n", w*x);

  //a)
  w = 10;
  printf("a)\n");
  printf("w: %d\n", w);
  printf("w: %d\n", ++w);

  //b)
  w = 10;
  printf("b)\n");
  printf("w: %d\n", ++w);
  printf("w: %d\n", w++);

  //c)
  w = 10;
  printf("c)\n");
  printf("w: %d\n", ++w);
  printf("w: %d\n", ++w);

   //d)
  w = 10;
  printf("d)\n");
  w+=5;
  printf("w: %d\n", w);


return 0;




}
