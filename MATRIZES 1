#include <stdio.h>

int main(){

int i, j, m[2][2];                                                                      //escreva todos os n√∫meros dessa matriz na tela



    printf("Por favor, digite os elementos da matriz:\n");
     for(i=1; i<=2; i++){
        for(j=1;j<=2;j++){
          scanf("%d", &m[i][j]);
   }
 }

       for(i=1; i<=2; i++){
          for(j=1;j<=2;j++){
            printf("Seus elementos: %d\n", m[i][j]);
          }
       }

system("pause");
}
